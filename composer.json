    "type": "library",
    "keywords": [
        "ckeditor",
        "fckeditor",
        "editor",
        "wysiwyg",
        "html",
        "richtext",
        "text",
        "javascript",
        "laravel"
    ],
    "homepage": "http://ckeditor.com",
    "license": [
        "GPL-2.0+",
        "LGPL-2.1+",
        "MPL-1.1+"
    ],
    "authors": [
        {
            "name": "CKSource",
            "homepage": "http://cksource.com"
        },
        {
            "name": "TechGuy. (Composer Package Maintainer)",
